    <style>
        .backTop {
            display: block;
            width: 40px;
            height: 40px;
            border-radius: 50%;
            background-color: #CC2121;
            text-align: center;
            position: fixed;
            right: 2%;
            bottom: 20%;
            z-index: 9999999;
            display: none;
        }

        .backTop:hover{
            background-color: #111;
            transition: all .5s;
        }

        .lbo .swiper-container {
            width: 100%;
            height: 100%;
            margin-left: auto;
            margin-right: auto;
        }

        .lbo .swiper-slide {
            text-align: center;
            font-size: 18px;
            background: #fff;

            /* Center slide text vertically */
            display: -webkit-box;
            display: -ms-flexbox;
            display: -webkit-flex;
            display: flex;
            -webkit-box-pack: center;
            -ms-flex-pack: center;
            -webkit-justify-content: center;
            justify-content: center;
            -webkit-box-align: center;
            -ms-flex-align: center;
            -webkit-align-items: center;
            align-items: center;
        }

        .lbo .swiper-container img {
            width: 100%;
            height: 100%;
        }

        .lbo-main span {
            position: absolute;
            display: block;
            color: #CC2121;
            font-size: 30px;
            line-height: 1.2;
            padding-bottom: 15px;
            text-transform: capitalize;
            font-family: "Yesteryear", cursive;
            top: 20%;
            left: 5%;
        }

        .lbo-main h1,
        .lbo-main h2 {
            position: absolute;
            color: #111;
            font-size: 32px;
            font-weight: 500;
            line-height: 1.1;
            text-transform: capitalize;

        }

        .lbo-main h1 {
            left: 5%;
            top: 30%;
        }

        .lbo-main h2 {
            left: 5%;
            top: 40%;
        }

        a {
            text-decoration: none;
        }

        .btn-hero {
            position: absolute;
            color: #fff;
            font-size: 14px;
            font-weight: 500;
            line-height: 1;
            padding: 18px 36px;
            border-radius: 3px;
            display: inline-block;
            background-color: #CC2121;
            text-transform: capitalize;
            border-radius: 0;
            left: 8%;
            top: 55%;
        }

        .swiper-pagination,
        .swiper-button-next,
        .swiper-button-prev {
            display: none;
        }

        .foot li a:hover {
            color: #CC2121;
            transform: translateX(10px);
            transition: all 0.5s linear;
        }
    </style>

</head>

